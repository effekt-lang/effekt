import examples/benchmarks/runner

def range(i: Int, n: Int): List[Int] =
  if (i < n) {
    Cons(i, range(i + 1, n))
  } else {
    Nil()
  }

def sum(xs: List[Int]): Int =
  xs match {
    case Nil() => 0
    case Cons(y, ys) => y + sum(ys)
  }

def run(n: Int): Int = sum(range(0, n))

def main() = benchmark(10){run}
