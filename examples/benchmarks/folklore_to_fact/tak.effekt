import examples/benchmarks/runner

def tak(x: Int, y: Int, z: Int): Int =
  if (y < x) {
    tak(
      tak(x - 1, y, z),
      tak(y - 1, z, x),
      tak(z - 1, x, y)
    )
  } else {
    z
  }

def run(x: Int): Int = {
  // only one argument
  val y = x - 2
  val z = x - 6
  tak(x, y, z)
}

def main() = benchmark(14){run}