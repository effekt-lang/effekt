import ref

// md = Person requires 2 saving slots
record Person(
  field1: Int,
  field2: Int,
  field3: Int,
  field4: Int,
  field5: Int,
  field6: Int,
  field7: Int
  )

def main() = {
  val p1 = ref::unsafeAllocate()
  val p2 = Person(1, 2, 3, 4, 5, 6, 7)
  p1.set(p2)
  println(p1.get().field7)
}
