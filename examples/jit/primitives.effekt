def assert(b: Bool, msg: String) = {
  if(b) {
    println("OK")
  } else {
    println(msg)
  }
}
def assertNot(b: Bool, msg: String) = {
  if(b) {
    println(msg)
  } else {
    println("OK")
  }
}
def main() = {
    val start = currentTimeNanos();
    println(0);
    val x = 2;
    val y = 40;
    println(x+y);
    println(x*y);
    println(y/x);
    println(x-y);
    println(mod(x,y));
    assert(x<y, "<");
    assert(x<=y, "<=");
    assertNot(x==y, "==");
    assertNot(x>y, ">");
    assert(x != y, "!=");
    assertNot(x>=y, ">=");
    assert(random() != random(), "random");
    assert((currentTimeNanos()-start) > 0, "time did not pass");
}
