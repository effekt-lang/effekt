effect GID {
    def gid(): Int
}

def f(): Int / GID = {
    gid() + gid()
}
def g(): Int / GID = {
    0
}
def h(): Int / GID = {
    gid() + gid() + gid()
}

def main() = {
    var n = 0
    try {
        println(f())
        println(g())
        println(h())
    } with GID {
        def gid() = { n = n+1; resume(n) }
    }
}
