import process
import stringbuffer
def main() = {
  val p = spawn("cat", [].fromList, default().onStdout(box { c =>
    println(s"Printed '${c}'")
  }).onStderr(box { c =>
    println(s"Err '${c}'")
  }).withStdin(box { h =>
    h.write("Hallo");
    h.close();
  })
  )
  println(p)
  val p2 = spawn("echo", ["Hallo"].fromList, default())
  println(p2)
  ()
}