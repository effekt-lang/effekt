module examples/pos/list/splitat

import immutable/list

def test(list: List[Int], index: Int): Unit = {
  println("index: " ++ show(index))
  val (l, r) = list.splitAt(index)
  println(l)
  println(r)
  println("")
}

def main() = {
  val empty: List[Int] = Nil()

  test(empty, -1)
  test(empty, 0)
  test(empty, 1)

  val list: List[Int] = [1, 2, 3]

  test(list, -1)
  test(list, 0)
  test(list, 1)
  test(list, 2)
  test(list, 3)
  test(list, 4)
}